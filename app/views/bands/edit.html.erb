<h1>Edit Band</h1>

<form action="<%= band_url(@band) %>" method="post">
  <%= auth_token_input %>
  <input type="hidden" name="_method" value="patch">
  <label for="band_name">Name</label>
  <input type="text" name="band[name]" id="band_name" value="<%= @band.name %>">
  <br>
  <input type="submit" value="Edit Band">
</form>
